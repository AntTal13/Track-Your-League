<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Team Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% teams.forEach(function(t) { %>
      <tr>
        <td><%= t.name %></td>
        <td><a href="/teams/<%= t._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
